// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


limitTemperature
{
    type	limitTemperature;
    active	true;
    limitTemperatureCoeffs
    {
        selectionMode all;
        min	200;
        max	400;
    }
}
